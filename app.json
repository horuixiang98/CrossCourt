{
  "expo": {
    "name": "CrossCourt",
    "slug": "CrossCourt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "crosscourt",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.horuixiang.CrossCourt"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.horuixiang.CrossCourt"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.838938142724-jmp35j3tva45kg9v1mt192uskeir5ei3"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splashscreen/badminton_splashscreen.jpg",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/splashscreen/badminton_splashscreen.jpg",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "36d0b032-0adf-4f6a-ae36-1fddafb46c3c"
      }
    }
  }
}
