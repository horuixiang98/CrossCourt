{
  "expo": {
    "name": "菜就多练",
    "slug": "CrossCourt",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "crosscourt",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.horuixiang.CrossCourt"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.horuixiang.CrossCourt"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/android-icon-48x48.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.838938142724-jmp35j3tva45kg9v1mt192uskeir5ei3"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splashscreen/badminton_splashscreen.jpg",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/splashscreen/badminton_splashscreen.jpg",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-sqlite",
      "expo-web-browser",
      "expo-camera",
      "expo-location",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you set your avatar and club cover."
        }
      ],
      "expo-notifications",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "36d0b032-0adf-4f6a-ae36-1fddafb46c3c"
      }
    }
  }
}
